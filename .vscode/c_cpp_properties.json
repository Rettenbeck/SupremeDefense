                "${workspaceFolder}/**",
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/src",
                "${workspaceFolder}/lib",
                "${workspaceFolder}/include/SFML",
                "${workspaceFolder}/include/imgui-master",
                "${workspaceFolder}/include/imgui-sfml-master"
